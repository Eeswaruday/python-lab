a = 10
b = 5

print("Addition:", a + b)
print("Subtraction:", a - b)
print("Multiplication:", a * b)
print("Division:", a / b)
print("a > b:", a > b)

a += 3
print("a after += 3:", a)

print("Logical AND:", a > 5 and b < 10)
print("Bitwise AND:", a & b)

max_val = a if a > b else b
print("Max using ternary:", max_val)

print("5 in [1, 2, 3, 4, 5]:", 5 in [1, 2, 3, 4, 5])

x = 10
y = 10
print("x is y:", x is y)
