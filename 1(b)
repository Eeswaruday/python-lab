a = int(input("Enter starting value: "))
b = int(input("Enter stop value: "))

print("Prime numbers between", a, "and", b, "are:")

for num in range(a, b + 1):
    if num > 1:
        for i in range(2, int(num ** 0.5) + 1):
            if num % i == 0:
                break
        else:
            print(num)
